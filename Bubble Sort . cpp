class Solution {
  public:
    void bubbleSort(vector<int>& arr) {
        // code here
        int n=arr.size();
        int say=0;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n-1-i;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
                say=1;
            }
        }
        }
        return;
    }
};
